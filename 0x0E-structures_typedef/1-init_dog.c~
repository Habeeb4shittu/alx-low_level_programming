#include "dog.h"
#include <stdlib.h>
/**
* init_dog - initialize variables with a type of struct dog
* @d: pointer to a datatype of dog
* @name: pointer to the name of the dog
* @age: age of the dog
* @owner: pointer to the owner of the dog
* Return: Always success
**/
void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d == null)
{
return;
}
else
{
d->name = name;
d->age = age;
d->owner = owner;
}
}
